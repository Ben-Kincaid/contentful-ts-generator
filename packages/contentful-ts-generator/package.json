{
  "name": "contentful-ts-generator",
  "version": "0.1.0",
  "description": "A toolbox of utilities to help with async (promise-based) transformations",
  "main": "index.js",
  "scripts": {
    "test": "tsc --project ../../tsconfig.test.json && nyc ava 'src/**/*.test.js'",
    "test-watch": "ava --watch",
    "build": "tsc --project tsconfig.dist.json",
    "prepare": "npm run build",
    "clean": "rm -rf dist; bash -c 'shopt -s globstar; rm src/**/*.js src/**/*.map src/**/*.d.ts'; true"
  },
  "keywords": [
    "async",
    "stream",
    "monad"
  ],
  "author": "Gordon Burgett (gordon@gordonburgett.net)",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/globby": "^9.1.0",
    "@types/inflection": "^1.5.28",
    "@types/nock": "^9.3.1",
    "@types/node": "^11.11.3",
    "@types/webpack": "^4.4.25",
    "ava": "^1.2.0",
    "coveralls": "^3.0.2",
    "nock": "^10.0.6",
    "nyc": "^13.1.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.4"
  },
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "cache": true,
    "failFast": true,
    "verbose": true,
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "async-toolbox": "^0.2.4",
    "chalk": "^2.4.2",
    "contentful-management": "^5.7.0",
    "fs-extra": "^7.0.1",
    "globby": "^9.1.0",
    "inflection": "^1.12.0",
    "limiter": "^1.1.4",
    "ts-morph": "^1.3.0"
  }
}
